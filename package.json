  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node src/server",
    "start:dev": "NODE_ENV=test nodemon src/server",
    "migration:make": "NODE_ENV=test knex --knexfile src/config/knexfile.js migrate:make",
    "migration:latest": "NODE_ENV=test knex --knexfile src/config/knexfile.js migrate:latest",
    "migration:rollback": "NODE_ENV=test knex --knexfile src/config/knexfile.js migrate:rollback",
    "migration:run": "NODE_ENV=test knex --knexfile src/config/knexfile.js migrate:up",
    "seed:make": "NODE_ENV=test knex --knexfile src/config/knexfile.js seed:make",
    "seed:run": "NODE_ENV=test knex --knexfile src/config/knexfile.js seed:run"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "date-fns": "^2.22.1",
    "dotenv": "^9.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.4",
    "multer": "^1.4.2",
    "nodemailer": "^6.6.3",
    "objection": "^2.2.15",
    "pg": "^8.6.0",
    "shortid": "^2.2.16",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "^14.2.1",
